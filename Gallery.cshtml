@{
    Layout = "~/Views/Shared/_Layout1.cshtml";
    ViewData["Title"] = "Gallery Page";
}
<!-- Portfolio Start -->
<div class="container-fluid py-5 px-0">
    <div class="container py-5">
        <div class="row justify-content-center">
            <div class="col-lg-5">
                <h1 class="section-title position-relative text-center mb-5">Delicious Foods Made From Our Very Own Organic Fresh Food</h1>
            </div>
        </div>
        <div class="row">
            <div class="col-12 text-center">
                <ul class="list-inline mb-4 pb-2" id="portfolio-flters">
                    <li class="btn btn-sm btn-outline-primary m-1 active" data-filter="*">All</li>
                    <li class="btn btn-sm btn-outline-primary m-1" data-filter=".first">Pizza</li>
                    <li class="btn btn-sm btn-outline-primary m-1" data-filter=".second">Fast Food</li>
                    <li class="btn btn-sm btn-outline-primary m-1" data-filter=".third">Biryani</li>
                </ul>
            </div>
        </div>
        <div class="row m-0 portfolio-container">
            <div class="col-lg-4 col-md-6 p-0 portfolio-item first">
                <div class="position-relative overflow-hidden">
                    <img class="img-fluid w-100" src="~/foodpics/dammoimage/VEGETABLE PIZZA.jpg" alt="">
                    <a class="portfolio-btn" href="~/foodpics/dammoimage/VEGETABLE PIZZA.jpg" data-lightbox="portfolio">
                        <i class="fa fa-plus text-primary" style="font-size: 60px;"></i>
                    </a>
                </div>
            </div>
            <div class="col-lg-4 col-md-6 p-0 portfolio-item second">
                <div class="position-relative overflow-hidden">
                    <img class="img-fluid w-100" src="~/foodpics/menu-img/pizza fries.png" alt="">
                    <a class="portfolio-btn" href="~/foodpics/menu-img/pizza fries.png" data-lightbox="portfolio">
                        <i class="fa fa-plus text-primary" style="font-size: 60px;"></i>
                    </a>
                </div>
            </div>
            <div class="col-lg-4 col-md-6 p-0 portfolio-item third">
                <div class="position-relative overflow-hidden">
                    <img class="img-fluid w-100" src="~/foodpics/dammoimage/KALEJI.jpg" alt="">
                    <a class="portfolio-btn" href="~/foodpics/dammoimage/KALEJI.jpg" data-lightbox="portfolio">
                        <i class="fa fa-plus text-primary" style="font-size: 60px;"></i>
                    </a>
                </div>
            </div>
            <div class="col-lg-4 col-md-6 p-0 portfolio-item first">
                <div class="position-relative overflow-hidden">
                    <img class="img-fluid w-100" src="~/foodpics/dammoimage/BAR B Q.jpg" alt="">
                    <a class="portfolio-btn" href="~/foodpics/dammoimage/BAR B Q.jpg" data-lightbox="portfolio">
                        <i class="fa fa-plus text-primary" style="font-size: 60px;"></i>
                    </a>
                </div>
            </div>
            <div class="col-lg-4 col-md-6 p-0 portfolio-item second">
                <div class="position-relative overflow-hidden">
                    <img class="img-fluid w-100" src="~/foodpics/image/burger.jpg" alt="">
                    <a class="portfolio-btn" href="~/foodpics/image/burger.jpg" data-lightbox="portfolio">
                        <i class="fa fa-plus text-primary" style="font-size: 60px;"></i>
                    </a>
                </div>
            </div>
            <div class="col-lg-4 col-md-6 p-0 portfolio-item third">
                <div class="position-relative overflow-hidden">
                    <img class="img-fluid w-100" src="~/foodpics/dammoimage/CHICKEN-BIRYANI.jpg" alt="">
                    <a class="portfolio-btn" href="~/foodpics/dammoimage/CHICKEN-BIRYANI.jpg" data-lightbox="portfolio">
                        <i class="fa fa-plus text-primary" style="font-size: 60px;"></i>
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Portfolio End -->